<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spldeolin.beginningmind.mapper.SecurityAccessTokenMapper">
    <select id="listAll" resultType="com.spldeolin.beginningmind.entity.SecurityAccessTokenEntity">
        SELECT
            *
        FROM
            security_access_token
        WHERE
            is_deleted = FALSE
    </select>
</mapper>